<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.petapp1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PetAPP1">
        <activity
            android:name=".Chat"
            android:label="@string/title_activity_chat"
            android:theme="@style/Theme.PetAPP1.NoActionBar"></activity>
        <activity
            android:name=".FoodDetail"
            android:label="@string/title_activity_food_detail"
            android:theme="@style/Theme.PetAPP1.NoActionBar" />
        <activity
            android:name=".AddPetfood"
            android:label="@string/title_activity_add_petfood"
            android:theme="@style/Theme.PetAPP1.NoActionBar" />
        <activity
            android:name=".AdoptionDetail"
            android:label="@string/title_activity_adoption_detail"
            android:theme="@style/Theme.PetAPP1.NoActionBar" />
        <activity android:name=".DonationDetail" />
        <activity android:name=".GivePetActivity" />
        <activity android:name=".AdoptionActivity" />
        <activity android:name=".TakerActivity" />
        <activity android:name=".DonorActivity" />
        <activity android:name=".PetfoodActivity" />
        <activity android:name=".ForgotpassActivity" />
        <activity android:name=".RegistrationActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".FAQsActivity" />
        <activity android:name=".DonateActivity" />
        <activity android:name=".AdoptActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>